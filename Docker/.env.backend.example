# === Database ===
ConnectionStrings__DefaultConnection=Server=db;Database=AIGenVideo;User=sa;Password=<your-db-password>;TrustServerCertificate=True;MultipleActiveResultSets=true
ConnectionStrings__Redis=redis:6379

# === JWT Settings ===
JWT__Issuer=http://localhost:1
JWT__Audience=http://localhost:2
JWT__SigningKey=<your-jwt-signing-key>
JWT__RefeshTokenExpirationInMinutes=720
JWT__AccessTokenExpirationInMinutes=10

# === Token Settings ===
PasswordToken__LifeTimeInMinutes=60

# === Email Settings ===
EmailSettings__SmtpServer=smtp.gmail.com
EmailSettings__SmtpPort=587
EmailSettings__SenderName=AI Gen Video
EmailSettings__SenderEmail=<your-email>
EmailSettings__Password=<your-email-password>

# === Google OAuth ===
Authentication__Google__ClientId=<your-google-client-id>
Authentication__Google__ClientSecret=<your-google-client-secret>
Authentication__Google__RedirectUri=<your-google-redirect-uri>

# === TikTok OAuth ===
Authentication__TikTok__ClientId=<your-tiktok-client-id>
Authentication__TikTok__ClientSecret=<your-tiktok-client-secret>
Authentication__TikTok__RedirectUri=<your-tiktok-redirect-uri>

# === Facebook OAuth ===
Authentication__Facebook__ClientId=<your-facebook-client-id>
Authentication__Facebook__ClientSecret=<your-facebook-client-secret>
Authentication__Facebook__RedirectUri=<your-facebook-redirect-uri>

# === VNPay ===
Payment__VnPay__ReturnUrl=http://localhost:7073/api/payment/vnpay-return
Payment__VnPay__PaymentUrl=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
Payment__VnPay__TmnCode=<your-vnpay-tmn-code>
Payment__VnPay__HashSecret=<your-vnpay-hash-secret>
Payment__VnPay__Version=2.1.0

# === MoMo ===
Payment__Momo__PartnerCode=<your-momo-partner-code>
Payment__Momo__ReturnUrl=http://localhost:7073/api/payment/momo-return
Payment__Momo__PaymentUrl=https://test-payment.momo.vn/v2/gateway/api/create
Payment__Momo__IpnUrl=http://localhost:7073/api/payment/momo-ipn
Payment__Momo__AccessKey=<your-momo-access-key>
Payment__Momo__SecretKey=<your-momo-secret-key>

# === AI / API Keys ===
Gemini__ApiKey=<your-gemini-api-key>
ApiKeys__YouTube=<your-youtube-api-key>
ApiKeys__Gemini=<your-gemini-api-key>
ApiKeys__Groq=<your-groq-api-key>
ApiKeys__AssemblyAI=<your-assemblyai-api-key>

# === Google Cloud ===
GoogleCloud__CredentialsPath=google-credentials.json
GoogleCloud__DefaultVoice=en-US-Standard-B
